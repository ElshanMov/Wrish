@model List<Faq>
@{
    ViewData["Title"] = "FAQ";
    int count = 0;
}

<main>
    <section id="faqs">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="faqs-page-title">
                        <h1>Frequently Asked Questions<span>.</span></h1>
                    </div>
                </div>
                @foreach (var item in Model)
                {
                    count++;
                    <div class="col-lg-6 col-md-6 col-sm-12">
                        <div class="question-content">

                            <h3 class="question-title">0@(count). @item.QuestionsTitle</h3>
                            <div class="faq_editor">@Html.Raw(item.Content)</div>
                        </div>
                    </div>
                }


            </div>
        </div>
    </section>
</main>

